{
  "name": "posenet-experiments",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@serialport/bindings": "^8.0.6",
    "@tensorflow-models/posenet": "^2.1.3",
    "@tensorflow/tfjs": "^1.1.0",
    "@tensorflow/tfjs-converter": "^1.3.0",
    "@tensorflow/tfjs-core": "^1.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "osc": "^2.3.1",
    "seedrandom": "~2.4.3",
    "stats.js": "^0.17.0",
    "ws": "^7.4.6"
  },
  "scripts": {
    "api-server": "cross-env NODE_ENV=development babel-node server.js",
    "parcel:watch": "cross-env NODE_ENV=development parcel index.html --no-hmr --open",
    "start": "concurrently npm:api-server npm:parcel:watch",
    "build": "cross-env NODE_ENV=production parcel build index.html --no-minify --public-url ./",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.7.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "dat.gui": "^0.7.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.19.1"
  },
  "eslintConfig": {
    "extends": "prettier",
    "rules": {
      "max-len": [
        "warn",
        {
          "code": 120
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "require-jsdoc": 0,
      "valid-jsdoc": 0
    },
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "dist/"
  ],
  "prettier": {
    "printWidth": 88,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
